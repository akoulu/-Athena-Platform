{
  "name": "api-client",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/org/data-access/api-client/src",
  "projectType": "library",
  "tags": ["scope:org", "type:lib", "type:data-access"],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/org/data-access/api-client",
        "main": "libs/org/data-access/api-client/src/index.ts",
        "tsConfig": "libs/org/data-access/api-client/tsconfig.lib.json",
        "assets": ["libs/org/data-access/api-client/*.md"],
        "format": ["cjs"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/org/data-access/api-client/jest.config.ts"
      }
    }
  }
}
