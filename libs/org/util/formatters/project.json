{
  "name": "formatters",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/org/util/formatters/src",
  "projectType": "library",
  "tags": ["scope:org", "type:lib", "type:util"],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/org/util/formatters",
        "main": "libs/org/util/formatters/src/index.ts",
        "tsConfig": "libs/org/util/formatters/tsconfig.lib.json",
        "assets": ["libs/org/util/formatters/*.md"],
        "format": ["cjs"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/org/util/formatters/jest.config.ts"
      }
    }
  }
}
